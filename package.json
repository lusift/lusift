{
  "name": "lusift",
  "version": "1.0.0",
  "sideEffects": false,
  "private": true,
  "description": "",
  "browser": "dist/index.js",
  "main": "index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./react": {
      "require": "./dist/react/src/index.js",
      "types": "./dist/react/src/index.d.ts",
      "default": "./dist/react/src/index.js"
    },
    "./vue": {
      "require": "./dist/vue/src/index.js",
      "types": "./dist/vue/src/index.d.ts",
      "default": "./dist/vue/src/index.js"
    },
    "./package.json": "./package.json",
    "./dev/react": {
      "require": "./dev/react/src/index.js",
      "types": "./dev/react/src/index.d.ts",
      "default": "./dev/react/src/index.js"
    }
  },
  "typesVersions": {
    "*": {
      "react": [
        "dist/react/src/index.d.ts"
      ],
      "vue": [
        "dist/vue/src/index.d.ts"
      ]
    }
  },
  "files": [
    "dist",
    "dev"
  ],
  "author": "",
  "license": "ISC",
  "bugs": "https://github.com/nareshbhusal/lusift/issues",
  "homepage": "https://lusift.com/",
  "keywords": [],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nareshbhusal/lusift.git"
  },
  "scripts": {
    "dev": "NODE_ENV=development rollup -c config/rollup.config.js -w",
    "build": "NODE_ENV=production rollup -c config/rollup.config.js",
    "build:watch": "npm run build -- -w",
    "test": "jest --watchAll",
    "lint": "npm run lint:eslint src/*",
    "lint:eslint": "eslint --ignore-path .gitignore",
    "format": "prettier --write 'src/**/*.(ts|js|tsx|jsx|vue|css)'",
    "vue-build": "NODE_ENV=production rollup -c config/rollup.config.vue.js",
    "vue-dev": "NODE_ENV=development rollup -c config/rollup.config.vue.js -w",
    "clean": "rimraf dist",
    "__(prepare runs on lusift package install)prepare": "npm run clean && npm run build"
  },
  "devDependencies": {
    "@babel/core": "7.16.5",
    "@babel/plugin-transform-runtime": "7.16.5",
    "@babel/preset-env": "7.16.5",
    "@babel/preset-react": "7.16.5",
    "@babel/preset-typescript": "7.16.5",
    "@babel/runtime": "7.16.5",
    "@rollup/plugin-alias": "3.1.8",
    "@rollup/plugin-babel": "5.3.0",
    "@rollup/plugin-commonjs": "21.0.1",
    "@rollup/plugin-eslint": "8.0.1",
    "@rollup/plugin-node-resolve": "13.1.1",
    "@rollup/plugin-replace": "3.0.0",
    "@rollup/plugin-strip": "2.1.0",
    "@rollup/plugin-typescript": "8.3.0",
    "@types/jest": "27.0.1",
    "@types/node": "^12.12.38",
    "@types/react": "16.4.7",
    "@types/react-dom": "^16.9.7",
    "@typescript-eslint/eslint-plugin": "4.31.1",
    "@typescript-eslint/parser": "4.31.1",
    "@vue/babel-preset-app": "4.5.15",
    "@vue/cli-plugin-babel": "4.5.15",
    "babel-eslint": "^10.0.3",
    "css-loader": "6.5.1",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-prettier": "4.0.0",
    "jest": "27.2.0",
    "prettier": "2.4.1",
    "rimraf": "3.0.2",
    "rollup": "2.61.1",
    "rollup-plugin-peer-deps-external": "2.2.4",
    "rollup-plugin-postcss": "4.0.2",
    "rollup-plugin-source-map-explorer": "1.1.1",
    "rollup-plugin-terser": "7.0.2",
    "rollup-plugin-typescript2": "0.31.1",
    "rollup-plugin-visualizer": "5.5.2",
    "rollup-plugin-vue": "6.0.0",
    "ts-jest": "27.0.5",
    "ts-loader": "9.2.6",
    "ts-node": "10.2.1",
    "tslib": "2.3.1",
    "tslint": "6.1.3",
    "typescript": "4.4.3",
    "webpack-cli": "4.9.1"
  },
  "dependencies": {
    "@floating-ui/dom": "file:../floating-ui-tooltip/node_modules/@floating-ui/dom",
    "@vue/composition-api": "file:../vue-crash-2021/node_modules/@vue/composition-api",
    "floating-ui-tooltip": "file:../floating-ui-tooltip",
    "focus-trap": "6.7.1",
    "global": "4.4.0",
    "react": "file:../lusift-saas/client/node_modules/react",
    "react-dom": "file:../lusift-saas/client/node_modules/react-dom",
    "vue": "file:../vue-crash-2021/node_modules/vue"
  }
}
